{
  "name": "@zepdev/design-system-zsd-react",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/zepdev"
  },
  "engines": {
    "npm": ">=10.1.0",
    "node": ">=20.7.0"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:css": "tailwindcss -m -i ./src/tailwind.css -o ./dist/styles.css",
    "build:scripts": "vite build",
    "build:types": "tsc --p ./tsconfig.build.json",
    "preview": "vite preview",
    "storybook": "storybook dev -p 9008",
    "build-storybook": "storybook build",
    "build-storybook:develop": "cross-env NODE_ENV=development storybook build",
    "test-storybook": "test-storybook --url http://127.0.0.1:9008",
    "test": "vitest",
    "semantic-release": "semantic-release",
    "lint": "eslint --max-warnings=0 src",
    "prettier": "npx prettier --write .",
    "prepare": "install-peers",
    "chromatic": "chromatic --only-changed --exit-once-uploaded",
    "tailwind-config-viewer": "tailwind-config-viewer -c tailwind.config.cjs -o"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./dist/styles.css": "./dist/styles.css",
    "./tailwind.config": "./tailwind.config.cjs"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "tailwind.config.cjs"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^3.3.2",
    "@strapi/blocks-react-renderer": "^1.0.1",
    "@zepdev/design-system-component-library-react": "1.8.4",
    "@zepdev/figma-design-tokens": "^6.13.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "react-hook-form": "^7.47.0",
    "react-phone-number-input": "^3.3.7",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@zepdev/figma-design-tokens": "^6.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^7.5.0",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.0",
    "@storybook/cli": "^7.5.0",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.5.0",
    "@storybook/react-vite": "^7.5.0",
    "@storybook/test-runner": "^0.19.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react": "^4.1.0",
    "@zepdev/figma-design-tokens": "^6.13.0",
    "autoprefixer": "^10.4.16",
    "axe-playwright": "^1.2.3",
    "chromatic": "^9.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "install-peers-cli": "^2.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.39.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-preserve-directives": "^0.2.0",
    "semantic-release": "^22.0.5",
    "semantic-release-cli": "^5.4.6",
    "storybook": "^7.5.0",
    "tailwind-config-viewer": "^1.7.2",
    "tailwindcss": "^3.3.3",
    "tsc-alias": "^1.8.8",
    "typescript": "5.1.6",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-turbosnap": "^1.0.3",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zepdev/design-system-zsd-react.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "bugs": {
    "url": "https://github.com/zepdev/design-system-zsd-react/issues"
  },
  "homepage": "https://github.com/zepdev/design-system-zsd-react"
}
